<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="600.0" spacing="30.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReportsContoller">
   <children>
      <Label text="Reports" />
      <Separator prefWidth="200.0" />
      <TableView prefHeight="200.0" prefWidth="200.0" fx:id="reportTable">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#handleAppointmentsByTypeMonth" text="Appointments by Type and Month" />
       <Button mnemonicParsing="false" onAction="#handleQuantityofAppointments" text="total appointments per user" />

        <ComboBox onAction="#handleContactSchedule" promptText="Choose Contact" fx:id="ContactBox" />


   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="30.0" />
   </padding>
</VBox>
